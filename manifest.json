{
  "manifest_version": 2,
  "name": "DHCP release check",
  "description": "This extension get dhcp release status from yamaha rtx-810",
  "version": "1.0",
  "icons": {
    "16": "icon/icon_16.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon_19.png",
      "38": "icon/icon_38.png"
    },
    "default_title": "DHCP release checker",
    "default_popup": "popup.html"
  },
 "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "storage"
  ]
}